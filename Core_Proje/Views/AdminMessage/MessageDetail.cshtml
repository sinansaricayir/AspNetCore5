@model EntityLayer.Concrete.WriterMessage

@{
    ViewData["Title"] = "MessageDetail";
    Layout = "~/Views/Shared/_NewAdminLAyout.cshtml";
}


<div class="col-12 grid-margin stretch-card">
    <div class="card">
        <div class="card-body">

            @Html.Label("Konu Başlığı")
            @Html.TextBoxFor(x => x.Subject, new { @class = "form-control" })

            <br />

            @Html.Label("Mesaj içeriği")
            @Html.TextAreaFor(x => x.MessageContent, 10, 3, new { @class = "form-control" })

            <br />

            <a asp-action="Index" class="btn btn-outline-success">Yazar Admin Paneli</a>

        </div>

    </div>
</div>



